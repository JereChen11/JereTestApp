<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBarCustomView">
        <attr name="titleText" format="string" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="titleBackIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="PersonalInfoItemCustomView">
        <attr name="categoryText" format="string" />
        <attr name="contentText" format="string" />
        <attr name="isDisplayImage" format="boolean" />
        <attr name="imageResource" format="reference" />
    </declare-styleable>

    <declare-styleable name="SetPersonalInfoTitleBarCustomView">
        <attr name="titleContentText" format="string" />
    </declare-styleable>

    <declare-styleable name="MyAccountInfoItemCustomView">
        <attr name="categoryItemText" format="string" />
        <attr name="iconResource" format="reference" />
    </declare-styleable>

    <declare-styleable name="BottomBarItemCustomView">
        <attr name="bottomBarIconResource" format="reference" />
        <attr name="bottomBarContentText" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomCollectView">
        <attr name="isCollect" format="boolean" />
    </declare-styleable>
</resources>