<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white_smoke"
        tools:context=".account.moreinfo.SetGenderActivity">

        <com.jere.test.util.customcomponent.SetPersonalInfoTitleBarCustomView
            android:id="@+id/setGenderTitleBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:titleContentText="@string/set_gender_cn" />

        <Spinner
            android:id="@+id/selectGenderSpinner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:background="@color/dark_gray"
            android:spinnerMode="dropdown"
            app:layout_constraintTop_toBottomOf="@id/setGenderTitleBar" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>